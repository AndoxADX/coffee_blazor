            "cwd": "${workspaceFolder}/src/HCB.Internal.Web.Profile",
            "url": "http://localhost:5000"
        },
        {
            "name": "Internal.Web.RCL.Gallery",
            "type": "blazorwasm",
            "request": "launch",
